{
  "problemMatcher": [
    {
      "owner": "editorconfig-checker",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([^\\s].*):$",
          "file": 1
        },
        {
          "regexp": "^\\s*(\\d+):\\s*(.+)$",
          "line": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
